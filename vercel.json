{
  "version": 2,
  "builds": [
    {
      "src": "savannah_test/savannah_test/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "savannah_test/savannah_test/wsgi.py"
    }
  ],
  "env": {
    "DEBUG": "False",
    "DJANGO_SETTINGS_MODULE": "savannah_test.settings"
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.11"
    }
  }
}